{
  "name": "@mortezakarimi/nestjs-url-generator",
  "version": "1.0.0",
  "description": "NestJs library for generating & signing url",
  "keywords": [
    "nestjs",
    "nest",
    "generate-url",
    "signed-url",
    "sign-url"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cd sample && npm install $(npm pack ../ | tail -1)",
    "update": "npx npm-check-updates -u && cd sample && npx npm-check-updates -u",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{lib,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mortezakarimi/nestjs-url-generator.git"
  },
  "author": "Vong Panha Huot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mortezakarimi/nestjs-url-generator/issues"
  },
  "homepage": "https://github.com/mortezakarimi/nestjs-url-generator#readme",
  "devDependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@types/qs": "^6.9.9",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.2.0",
    "@nestjs/core": "^10.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "qs": "^6.11.2"
  }
}
